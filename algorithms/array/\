class removeDupSortedArray:
    # @param a list of integers
    # @return an integer
    def removeDuplicates(self, A):
        result = 1
        if A is None or len(A) == 0:
            return 0 
        pre = A[0]
        for i in xrange(1,len(A)):
            if A[i] == pre:
                continue
            pre = A[i]
            result += 1
        return result
        
print removeDupSortedArray().removeDuplicates([112])   
